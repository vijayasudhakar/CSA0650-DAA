#include<stdio.h> 
#include<conio.h>
int main() 
{ 
	int rows,i,j,k,num=10; 
	printf("P. Vijaya Lakshmi, Reg.No - 192210200\n");
	printf("\nEnter the Rows:"); 
	scanf("%d",&rows); 
    for(i=1;i<=rows;i++){  
        for(j=i;j<=rows-1;j++){  
            printf(" ");   
        }  
        for(k=1;k<=i;k++){  
            printf("%d  ",k);  
        }    
        printf("\n");  
    }     
	return 0; 
}
