#include<stdio.h>
#include<conio.h>
int main(){
	int a[10][10],b[10][10],i,j,k,c[10][10],n;
	printf("P. Vijaya Lakshmi, Reg.No - 192210200");
	printf("\nEnter index of matrix A and B:");
	scanf("%d",&n);
	printf("\nEnter the Elements in the Matrix A:");
	for(i=0;i<n;i++){
		for(j=0;j<n;j++){
			scanf("%d",&a[i][j]);
		}
	}
	printf("\nEnter the Elements in the Matrix B:");
	for(i=0;i<n;i++){
		for(j=0;j<n;j++){
			scanf("%d",&b[i][j]);
		}
	}
	for(i=0;i<n;i++){
		for(j=0;j<n;j++){
			c[i][j]=0;
			for(k=0;k<n;k++){
				c[i][j] = c[i][j] + a[i][k] * b[k][j];
			}
		}
	}
	printf("\nMultiplication of matrix A and B:\n");
	for(i=0;i<n;i++){
		for(j=0;j<n;j++){
			printf("%d ",c[i][j]);
		}
		printf("\n");
	}
	return 0;
}
